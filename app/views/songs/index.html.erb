<h1>Songs:</h1>


<table style="width: 100px">
        <tr>
            <th>Title</th>
            <th>Year</th>
            <th>Genre</th>
            <th>Artist</th>
        </tr>

        <% @songs.each do |song| %>
            <tr>
                <td><%= song.title %></td>
                <td><%= song.release_year%></td>
                <td><%= song.genre%></td>
                <td><%= song.artist_name%></td>
            </tr>
        <% end %>
</table>